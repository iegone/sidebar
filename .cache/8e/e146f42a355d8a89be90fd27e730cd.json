 100vw;\n  height: 100vh;\n}\n.container {\n  @include container();\n}\nsection {\n  @include section();\n}\nsection:nth-child(1) {\n  background-color: $main-color;\n}\n// sidebar start\n.sidebar {\n  position: absolute;\n  top: 0;\n  left: 0;\n  height: 100vh;\n  width: 80px;\n  background-color: $main-color4;\n  padding: 0.4rem 0.8rem;\n  transition: all 0.5s ease;\n  display: flex;\n  flex-direction: column;\n  justify-content: space-between;\n}\n.sidebar.active ~ .main-content {\n  left: 250px;\n  width: calc(100% - 250px);\n}\n.sidebar.active {\n  width: 250px;\n}\n.sidebar #but {\n  color: $main-color;\n  position: absolute;\n  top: 0.4rem;\n  left: 50%;\n  font-size: 1.2rem;\n  line-height: 50px;\n  transform: translateX(-50%);\n  cursor: pointer;\n}\n.sidebar.active #but {\n  left: 90%;\n}\n.sidebar .top .logo {\n  color: $main-color;\n  display: flex;\n  height: 50px;\n  width: 100%;\n  align-items: center;\n  pointer-events: none;\n  opacity: 0;\n}\n.sidebar.active .top .logo {\n  opacity: 1;\n}\n.top .logo i {\n  font-size: 2rem;\n  margin-right: 5px;\n}\n.sidebar ul li {\n  position: relative;\n  list-style-type: none;\n  height: 50px;\n  width: 90%;\n  margin: 0.8rem auto;\n  line-height: 50px;\n}\n.sidebar ul li a {\n  color: $main-color;\n  display: flex;\n  align-items: center;\n  text-decoration: none;\n  border-radius: 0.8rem;\n}\n.sidebar ul li a:hover {\n  background-color: $main-color;\n  color: $main-color4;\n}\n.sidebar ul li a i {\n  min-width: 50px;\n  text-align: center;\n  height: 50px;\n  border-radius: 12px;\n  line-height: 50px;\n}\n.sidebar .nav-item {\n  opacity: 0;\n}\n.sidebar.active .nav-item {\n  opacity: 1;\n}\n.sidebar ul li .tooltip {\n  position: absolute;\n  left: 125px;\n  top: 50%;\n  transform: translate(-50%, -50%);\n  box-shadow: 0 0.5rem 0.8rem rgba(0, 0, 0, 0.2);\n  border-radius: 6rem;\n  padding: 0.4rem 1.2rem;\n  line-height: 1.8rem;\n  z-index: 20;\n  opacity: 0;\n  background-color: $main-color;\n}\n.sidebar ul li:hover .tooltip {\n  opacity: 1;\n}\n.sidebar.active ul li .tooltip {\n  display: none;\n}\n// sidebar end\n.main-content {\n  background: url(../scss/sonoma.jpg);\n  background-repeat: no-repeat;\n  background-size: auto 100vh;\n  // background-color: $main-color3;\n  position: relative;\n  min-height: 100%;\n  top: 0;\n  left: 80px;\n  transition: all 0.5s ease;\n  width: calc(100% - 80px);\n  padding: 1rem;\n}\n\n.frem {\n  background-color: rgba(27, 27, 27, 0.9);\n  backdrop-filter: blur(6px);\n  height: 70vh;\n  width: 70vw;\n  border-radius: 10px;\n  \n}\n.uper-bar {\n  display: flex;\n  align-items: center;\n  background-color: rgb(57, 57, 57);\n  height: 30px;\n  border-radius: 10px 10px 0 0;\n}\n\n.cercel1,\n.cercel2,\n.cercel3 {\n  height: 12px;\n  width: 12px;\n  margin: 4px;\n  border-radius: 12px;\n}\n.cercel1 {\n  background-color: #ff5f57;\n  margin-left: 15px;\n}\n.cercel2 {\n  background-color: #febc2e;\n}\n.cercel3 {\n  background-color: #28c840;\n}\n.cercel1:hover,\n.cercel2:hover,\n.cercel3:hover {\n  background-color: bisque;\n}\n.windo {\n  color: aqua;\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  padding: 14px;\n}\n\n.typing {\n  color: antiquewhite;\n  white-space: nowrap;\n  overflow: hidden;\n  width: 150px;\n  border-right: 3px solid;\n  animation: typing 1.5s steps(13), cursor 0.4s step-end infinite alternate;\n}\n@keyframes cursor {\n  50% {\n    border-color: transparent;\n  }\n}\n@keyframes typing {\n  from {\n    width: 0px;\n  }\n}\n.windo-rest {\n  background: rgb(26, 142, 242);\n}\n\nsection:nth-child(2) {\n  background-color: $main-color2;\n}\n"},"lineCount":null}},"error":null,"hash":"9995e9b36829ea9898dfda1d8432bbd8","cacheData":{"env":{}}}